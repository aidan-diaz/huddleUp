# Convex Configuration
# Get this from your Convex dashboard after running `npx convex dev`
VITE_CONVEX_URL=

# Clerk Authentication (get from https://dashboard.clerk.com)
VITE_CLERK_PUBLISHABLE_KEY=

# ========== VIDEO/AUDIO CALLS (LiveKit) ==========
# Get your LiveKit URL from https://cloud.livekit.io or run locally
# For local development: wss://localhost:7880
VITE_LIVEKIT_URL=

# LiveKit API credentials (set in Convex dashboard under Environment Variables)
# These are used server-side in Convex to generate tokens
# LIVEKIT_API_KEY=
# LIVEKIT_API_SECRET=

# ========== PUSH NOTIFICATIONS (VAPID) ==========
# Generate VAPID keys: npx web-push generate-vapid-keys
# Public key goes here (client-side)
VITE_VAPID_PUBLIC_KEY=

# Private key goes in Convex Environment Variables (server-side)
# VAPID_PUBLIC_KEY=
# VAPID_PRIVATE_KEY=

# ========== OPTIONAL ==========
# Sentry error tracking
# VITE_SENTRY_DSN=
